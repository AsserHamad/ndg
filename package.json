{
  "name": "herokutrial",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "nodemon": "nodemon ./bin/www",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run nodemon\" \"cd client && npm start\""
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.17.3",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.11",
    "morgan": "~1.9.1",
    "nodemailer": "^6.4.10",
    "query-string": "^6.13.5",
    "react-html-parser": "^2.0.2",
    "react-quill": "^1.3.5"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
